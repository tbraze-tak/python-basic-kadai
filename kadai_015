class Human() :
    #初期化
    def __init__(self):
      self.name=""
      self.age = ""

    #メソッドの定義
    def set_human(self, name, age):
      self.name = name
      self.age = age

    def printinfo(self):
      print(self.name)
      print(self.age)


#インスタンス化
human = Human()

#メソッドへのアクセス
human.set_human("Tim", 45)
human.printinfo()
